{
  "name": "grubbrrapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4204",
    "start:test-local": "ng serve --port 4204 --configuration test-local",
    "build": "ng build --configuration=development",
    "build:dev": "ng build --configuration=dev",
    "build:test": "ng build --configuration=test",
    "build:sandbox": "ng build --configuration=sandbox",
    "build:stage": "ng build --configuration=stage",
    "build:production": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "proto": "protoc --ts_out src/app/grubbrr/generated/ --ts_opt ts_nocheck --ts_opt eslint_disable --ts_opt add_pb_suffix --ts_opt long_type_string --ts_opt generate_dependencies --proto_path ../Nge.Api/Areas/Portal/Models/Protos ../Nge.Api/Areas/Portal/Models/Protos/*.proto",
    "lint": "ng lint",
    "prettier:check": "prettier --check src",
    "prettier:format": "prettier --write src",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org noble --project nextgen-portal ./dist && sentry-cli sourcemaps upload --org noble --project nextgen-portal ./dist"
  },
  "private": true,
